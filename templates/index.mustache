<!DOCTYPE html>
<html>
<head>
	<title>beta.aintaer.com</title>
	{{> head}}
	<link rel="stylesheet" href="/css/index.css">
</head>
<body>
	{{> navigation}}

	{{#pagination}}
	<aside class="quicknav">
		{{#prev}}<a href="{{path}}" class="left icon-arrow-left5"></a>{{/prev}}
		{{#next}}<a href="{{path}}" class="right icon-arrow-right5"></a>{{/next}}
	</aside>
	{{/pagination}}

	{{{contents}}}

	{{# articles}}
	<article>
		<h1 data-date="{{dateFormat date}}"><a href="/{{filename}}">{{title}}</a></h1>
		<section>{{{excerpt}}}</section>
	</article>
	{{/ articles}}
	{{> footer}}
</body>
</html>
